# customcategory=calendars=calendars
themes.configuration {
	# customsubcategory=calendars_container=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_container
	container {
		# cat=calendars,container/calendars_container/; type=int+; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.container_calendar
		calendar = 0
	}
	# customsubcategory=calendars_pages=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_pages
	pages {
		calendar {
			# cat=calendars,pages/calendars_pages/; type=int+; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.pages_calendar_list
			list = 0
			# cat=calendars,pages/calendars_pages/; type=int+; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.pages_calendar_detail
			detail = 0
			# cat=calendars,pages/calendars_pages/; type=int+; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.pages_calendar_year
			year = 0
			# cat=calendars,pages/calendars_pages/; type=int+; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.pages_calendar_registration
			registration = 0
		}
	}
	extension {
		# customsubcategory=calendars_general=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_general
		calendars {
			# customsubcategory=calendars_registration=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_registration
			registration {
				# cat=calendars/calendars_registration/100; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_subject
				subject = Event-Anmeldung
				# cat=calendars/calendars_registration/200; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_to_email
				toEmail = typo3@coding.ms
				# cat=calendars/calendars_registration/300; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_to_name
				toName = Thomas Deuling
				# cat=calendars/calendars_registration/400; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_from_email
				fromEmail = typo3@coding.ms
				# cat=calendars/calendars_registration/500; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_from_name
				fromName = Event-Registrierung
				# cat=calendars/calendars_registration/600; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_bcc_email
				bccEmail =
				# cat=calendars/calendars_registration/700; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.registration_bcc_name
				bccName = Event-Registrierung (BCC)
				# cat=calendars/calendars_registration/800; type=boolean; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.enable_confirmation_email
				enableConfirmationEmail = 0
				# cat=calendars/calendars_registration/900; type=boolean; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.enable_redirect_after_registration
				enableRedirectAfterRegistration = 0
			}
			# customsubcategory=calendars_view=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_view
			view {
				# cat=calendars/calendars_view/100; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.view_template_root_path
				templateRootPath = EXT:calendars/Resources/Private/Templates/
				# cat=calendars/calendars_view/200; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.view_partial_root_path
				partialRootPath = EXT:calendars/Resources/Private/Partials/
				# cat=calendars/calendars_view/300; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.view_layout_root_path
				layoutRootPath = EXT:calendars/Resources/Private/Layouts/
			}
			# cat=calendars/calendars_general/100; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.default_image
			defaultImage = EXT:calendars/Resources/Public/Images/no-image.png
			# customsubcategory=calendars_list=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_list
			list {
				image {
					# cat=calendars/calendars_list/100; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.list_image_width
					width = 200
					# cat=calendars/calendars_list/200; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.list_image_height
					height = 150
				}
			}
			# customsubcategory=calendars_detail=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_detail
			detail {
				image {
					# cat=calendars/calendars_detail/100; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.detail_image_width
					width = 320
					# cat=calendars/calendars_detail/200; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.detail_image_height
					height = 320
				}
			}
			# customsubcategory=calendars_year=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_year
			year {
				# cat=calendars/calendars_year/100; type=options[0,1]; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.year_calendar_week_visible
				calendarWeekVisible = 1
				# cat=calendars/calendars_year/100; type=options[0,1]; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.year_category_explanation_visible
				categoryExplanationVisible = 1
			}
		}
	}
	#############################################
	# Socialmedia constants
	#############################################
	#customcategory=socialmedia=Socialmedia
	socialmedia {
		# customsubcategory=socialmedia_twitter=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_twitter
		twitter {
			# cat=calendars,socialmedia/socialmedia_twitter/10; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_twitter_title
			title = Twitter
			# cat=calendars,socialmedia/socialmedia_twitter/20; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_twitter_css_class
			cssClass = fa fa-twitter rounded-circle
		}
		# customsubcategory=socialmedia_facebook=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_facebook
		facebook {
			# cat=calendars,socialmedia/socialmedia_facebook/10; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_facebook_title
			title = Facebook
			# cat=calendars,socialmedia/socialmedia_facebook/20; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_facebook_css_class
			cssClass = fa fa-facebook rounded-circle
		}
		# customsubcategory=socialmedia_email=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.subcategory_email
		email {
			# cat=calendars,socialmedia/socialmedia_email/10; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_email_title
			title = Email
			# cat=calendars,socialmedia/socialmedia_email/20; type=string; label=LLL:EXT:calendars/Resources/Private/Language/locallang_constants.xlf:tx_calendars_constants.socialmedia_email_css_class
			cssClass = fa fa-email rounded-circle
		}
	}
}
