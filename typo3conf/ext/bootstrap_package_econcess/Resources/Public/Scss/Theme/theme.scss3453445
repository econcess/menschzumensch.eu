	}
	.record-holder figcaption.caption {
		text-align: left;
	}
	.record-holder .textpic-above:not(.laenderauswahl) .gallery-item img {
		display: block;
		max-width: 30%;
		margin-left: 0;
	}
	.record-holder h3 {
		font-size:1.15rem
	}
	.record-holder h4 {
		font-size:1.05rem
	}
	.lang_pre_header {
		
	}
	.lang_pre_header .container {
		display:flex;	
	}
	.lang_pre_header .container > div {
		width:50%;	
		display:table;
	}
	.lang_pre_header .container > div > p {
		text-align: left;
		display: table-cell;
		vertical-align: middle;
		margin:auto;
	}
	.lang_pre_header .container > div + div > div {
		float:right;
	}
}
@include media-breakpoint-up(lg) { 
	.footer-section-meta {
		max-width:960px;
	}
}
